<?xml version="1.0"?>
<model deformetrica-min-version="3.0.0">
    <model-type>LongitudinalMetricLearning</model-type>
    <dimension>4</dimension>
    <initial-time-shift-std>10.</initial-time-shift-std>
    <initial-log-acceleration-std>0.3</initial-log-acceleration-std>
    <initial-noise-std>0.1</initial-noise-std>
    <deformation-parameters>
        <!--<exponential-type>one_dimensional</exponential-type>-->
        <concentration-of-timepoints>12</concentration-of-timepoints>
        <exponential-type>parametric</exponential-type>
        <number-of-sources>2</number-of-sources>
        <kernel-width>0.4</kernel-width>
    </deformation-parameters>
</model>
