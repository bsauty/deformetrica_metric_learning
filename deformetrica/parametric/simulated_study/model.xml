<?xml version="1.0"?>
<model deformetrica-min-version="3.0.0">
    <model-type>LongitudinalMetricLearning</model-type>
    <dimension>2</dimension>
    <initial-time-shift-std>10.</initial-time-shift-std>
    <initial-log-acceleration-std>0.6</initial-log-acceleration-std>
    <initial-noise-std>0.1</initial-noise-std>
    <deformation-parameters>
        <exponential-type>parametric</exponential-type>
        <number-of-timepoints>50</number-of-timepoints>
        <concentration-of-timepoints>25</concentration-of-timepoints>
        <exponential-type>parametric</exponential-type>
        <number-of-sources>0</number-of-sources>
        <kernel-width>0.3</kernel-width>
    </deformation-parameters>
</model>
