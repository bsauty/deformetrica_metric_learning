<?xml version="1.0"?>
<model deformetrica-min-version="3.0.0">

    <model-type>LongitudinalAtlas</model-type>

    <template>
        <object id="starman">
            <deformable-object-type>PolyLine</deformable-object-type>
            <kernel-type>exact</kernel-type>
            <kernel-width>1</kernel-width>
            <filename>data/ForInitialization_Template.vtk</filename>
        </object>
    </template>

    <deformation-parameters>
        <kernel-type>exact</kernel-type>
        <kernel-width>1</kernel-width>
        <concentration-of-timepoints>5</concentration-of-timepoints>
        <nb-sources>2</nb-sources>
    </deformation-parameters>

    <initial-control-points>data/ForInitialization_ControlPoints_0.1.txt</initial-control-points>
    <initial-momenta>data/ForInitialization_Momenta_0.05.txt</initial-momenta>

