<?xml version="1.0"?>
<model deformetrica-min-version="3.0.0">
    <model-type>Registration</model-type>
    <template>
        <object id="pointcloud">
            <deformable-object-type>Landmark</deformable-object-type>
            <kernel-width>30</kernel-width>
            <noise-std>0.1</noise-std>
            <filename>data/sourcePoints.vtk</filename>
        </object>
    </template>
    <deformation-parameters>
        <kernel-width>20</kernel-width>
        <kernel-type>exact</kernel-type>
        <number-of-timepoints>10</number-of-timepoints>
    </deformation-parameters>
</model>
